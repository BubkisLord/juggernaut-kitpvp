item replace entity @s hotbar.8 with written_book[written_book_content={title:"Timekeeper",author:"ยง9Server",pages:[\
[{"text": "Timekeeper\n\n","bold": true,"color": "gold","underlined": false},{"text": "This new kit holds powerful (and possibly overtuned) new abilities surrounding time manipulation.","bold": false,"color": "black"}],\
\
[{"text": "Rewind\n","bold": true,"color": "gold"},{"text": "Activated Ability\n\n","bold": true,"color": "gray","italic": true},{"text": "One of the Timekeeper's most powerful abilities, this allows him to rewind himself back in time across his whole path in the last 10 minutes. Rewinding time will rewrite the path, meaning that once you clear it the only way to write the path back is to walk forwards in time.","bold": false,"color": "black"}],\
\
[{"text": "Fast-Forward\n","bold": true,"color": "gold"},{"text": "Activated Ability\n\n","bold": true,"color": "gray","italic": true},{"text": "Speed up time, allowing you to go at immense speed but also meaning replenishment increases much faster.","bold": false,"color": "black"},{"text": "The more you use this ability, the faster you and the replenishment will go. You will rewind faster as well.","bold": false,"color": "black"}],\
\
[{"text": "Reset Time Scale\n","bold": true,"color": "gold"},{"text": "Activated Ability\n\n","bold": true,"color": "gray","italic": true},{"text": "This ability resets the effects of Fast-Forward.","bold": false,"color": "black"}],\
\
[{"text": "Pause Time\n","bold": true,"color": "gold"},{"text": "Activated Ability\n\n","bold": true,"color": "gray","italic": true},{"text": "For a short duration, time will be slowed, meaning runners cannot move quickly, replenishment goes slow, and abilities are on cooldown for longer.","bold": false,"color": "black"},{"text": "You will be able to resist the change in the flow of time, allowing you to move at a faster speed compared to others.","bold": false,"color": "black"},{"text": "This ability does not stack.","bold": false,"color": "black"}]\
]}]
